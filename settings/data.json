{
  "title": "Skitter &#149; Slideshow for anytime",
  "small_title": "Skitter",
  "description": "Skitter is a free jQuery slider responsive with a multiple animations and options.",
  "repo": "https://github.com/thiagosf/skitter",
  "nav": {
    "home": { "label": "Home", "link": "/" },
    "install": { "label": "Install", "link": "/install.html" },
    "options": { "label": "Options", "link": "/options.html" },
    "api": { "label": "API", "link": "/api.html" },
    "themes": { "label": "Themes", "link": "/themes.html" },
    "comments": { "label": "Comments", "link": "/comments.html" }
  },
  "options": {
    "velocity": {
      "label": "Velocity of animation",
      "default": "1",
      "accept": "0.0 to 2.0",
      "type": "float"
    },
    "interval": {
      "label": "Interval between transitions",
      "default": "2500",
      "accept": "integer",
      "type": "integer"
    },
    "animation": {
      "label": "Default animation",
      "default": "null or defined in &lt;a&gt; class",
      "accept": "...",
      "type": "string"
    },
    "numbers": {
      "label": "Numbers display",
      "default": "true",
      "accept": "true, false",
      "type": "boolean"
    },
    "navigation": {
      "label": "Navigation display",
      "default": "true",
      "accept": "true, false",
      "type": "boolean"
    },
    "label": {
      "label": "Label display",
      "default": "true",
      "accept": "true, false",
      "type": "boolean"
    },
    "easing_default": {
      "label": "Easing default",
      "default": "null",
      "accept": "ease type",
      "type": "string"
    },
    "thumbs": {
      "label": "Navigation with thumbs",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "hide_tools": {
      "label": "Hide numbers and navigation",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "fullscreen": {
      "label": "Fullscreen mode",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "xml": {
      "label": "Loading data from XML file",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "dots": {
      "label": "Navigation with dots",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "show_randomly": {
      "label": "Randomly sliders",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "onLoad": {
      "label": "Callback",
      "default": "null",
      "accept": "function",
      "type": "function"
    },
    "numbers_align": {
      "label": "Alignment of numbers/dots/thumbs",
      "default": "left",
      "accept": "center, left, right",
      "type": "string"
    },
    "preview": {
      "label": "Preview with dots",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "focus": {
      "label": "Focus slideshow",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "focus_position": {
      "label": "Position of button focus slideshow",
      "default": "center",
      "accept": "center, leftTop, rightTop, leftBottom, rightBottom",
      "type": "string"
    },
    "controls": {
      "label": "Option play/pause manually",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "controls_position": {
      "label": "Position of button controls",
      "default": "center",
      "accept": "center, leftTop, rightTop, leftBottom, rightBottom",
      "type": "string"
    },
    "enable_navigation_keys": {
      "label": "Enable navigation keys",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "with_animations": {
      "label": "Specific animations to be used in random mode.",
      "default": "[]",
      "accept": "[\"paralell\", \"glassBlock\", \"swapBars\"]",
      "type": "array"
    },
    "mouseOverButton": {
      "label": "Function call to go over the navigation buttons",
      "default": "function() { $(this).stop().animate({opacity:0.5}, 200); }",
      "accept": "function() { $(this).stop().animate({opacity:0.2}, 500); }",
      "type": "function"
    },
    "mouseOutButton": {
      "label": "Function call to go out the navigation buttons",
      "default": "function() { $(this).stop().animate({opacity:0.5}, 200); }",
      "accept": "function() { $(this).stop().animate({opacity:0.2}, 500); }",
      "type": "function"
    },
    "stop_over": {
      "label": "Stop animation to move mouse over it.",
      "default": "true",
      "accept": "true, false",
      "type": "boolean"
    },
    "auto_play": {
      "label": "Sets whether the slideshow will start automatically",
      "default": "true",
      "accept": "true, false",
      "type": "boolean"
    },
    "label_animation": {
      "label": "Label animation type",
      "default": "slideUp",
      "accept": "slideUp, left, right, fixed",
      "type": "string"
    },
    "imageSwitched": {
      "label": "Function call to change image",
      "default": "null",
      "accept": "function",
      "type": "function"
    },
    "progressbar": {
      "label": "Displays progress bar",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "progressbar_css": {
      "label": "CSS progress bar",
      "default": "false",
      "accept": "true, false",
      "type": "boolean"
    },
    "theme": {
      "label": "Slideshow theme",
      "default": "null",
      "accept": "minimalist, round, clean, square",
      "type": "string"
    }
  },
  "api": {
    "getAnimations": {
      "label": "Get available animations",
      "example": "$('.skitter-large').skitter('getAnimations')",
      "return": "array"
    },
    "setAnimation": {
      "label": "Set animation for all slides",
      "example": "$('.skitter-large').skitter('setAnimation', 'fade')",
      "return": "null"
    },
    "next": {
      "label": "Next slide",
      "example": "$('.skitter-large').skitter('next')",
      "return": "null"
    },
    "prev": {
      "label": "Previous slide",
      "example": "$('.skitter-large').skitter('prev')",
      "return": "null"
    }
  },
  "themes": {
    "default": {
      "label": "Default",
      "link": "/default-theme.html"
    },
    "clean": {
      "label": "Clean",
      "link": "/clean-theme.html"
    },
    "minimalist": {
      "label": "Minimalist",
      "link": "/minimalist-theme.html"
    },
    "round": {
      "label": "Round",
      "link": "/round-theme.html"
    },
    "square": {
      "label": "Square",
      "link": "/square-theme.html"
    }
  },
  "slides": {
    "default": [
      {
        "image": "/images/mountains/1-seascape-with-rocks-in-ocean.jpg",
        "label": "And after a long time, a new version of Skitter, now responsive. <a href=\"https://visualhunt.com/photo/7377/seascape-with-rocks-in-ocean/\" target=\"_blank\" class=\"btn btn-xs btn-primary\">Image credits</a>",
        "animation": "cubeSize"
      },
      {
        "image": "/images/mountains/2-utah-mountain-sky-nature-golden-hour-sunset.jpg",
        "label": "With lots of animations, create amazing slides. <a href=\"https://visualhunt.com/photo/10330/\" target=\"_blank\" class=\"btn btn-xs btn-primary\">Image credits</a>",
        "animation": "horizontal"
      },
      {
        "image": "/images/mountains/3-sand-mountain-clouds.jpg",
        "label": "Use navigation with points, arrows, or numbers. <a href=\"https://visualhunt.com/photo/5653/\" target=\"_blank\" class=\"btn btn-xs btn-primary\">Image credits</a>",
        "animation": "showBars"
      },
      {
        "image": "/images/mountains/4-landscape-with-tree-hills-and-lake.jpg",
        "label": "Place a progress bar between slides. <a href=\"https://visualhunt.com/photo/12320/landscape-with-tree-hills-and-lake/\" target=\"_blank\" class=\"btn btn-xs btn-primary\">Image credits</a>",
        "animation": "showBarsRandom"
      },
      {
        "image": "/images/mountains/5-aurora-borealis-over-mountains-at-night.jpg",
        "label": "Some themes for you to use, but you can create your own. <a href=\"https://visualhunt.com/photo/6185/aurora-borealis-over-mountains-at-night/\" target=\"_blank\" class=\"btn btn-xs btn-primary\">Image credits</a>",
        "animation": "tube"
      },
      {
        "image": "/images/mountains/6-lake-powell-page-arizona-water-reservoir-landscape.jpg",
        "label": "Finally: have fun! <a href=\"https://visualhunt.com/photo/13958/\" target=\"_blank\" class=\"btn btn-xs btn-primary\">Image credits</a>",
        "animation": "paralell"
      }
    ]
  }
}
